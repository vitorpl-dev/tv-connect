{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/socket",
      "dest": "http://localhost:8080/socket",
      "protocols": ["websocket"]
    },
    {
      "src": "/(.*)",
      "dest": "./index.js"
    }
  ]
}
