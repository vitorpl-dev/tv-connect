{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/socket/(.*)",
      "dest": "http://localhost:8080/socket/$1",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "./index.js"
    }
  ]
}
